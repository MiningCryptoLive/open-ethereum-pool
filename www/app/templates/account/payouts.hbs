<div class="container">
  <div class="row">
    <div class="col-xs-12 col-md-12">
	  <div class="note note-success">
	    {{high-charts mode=chartMode chartOptions=chartPayment content=chartData}}
	  </div>
	</div>
  </div>
</div>
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-md-12">
	  {{#if model.payments}}
	  <h4>Latest Payouts</h4>
		<div class="table-responsive">
		  <table class="table table-condensed table-striped">
			<thead>
			  <tr>
				<th>Time</th>
				<th>Tx ID</th>
				<th>Amount</th>
			  </tr>
			</thead>
			<tbody>
			  {{#each model.payments as |tx|}}
				<tr>
				  <td>{{format-date-locale tx.timestamp}}</td>
				  <td>
					<a href="https://expedition.dev/tx/{{tx.tx}}" class="hash" rel="nofollow" target="_blank">{{tx.tx}}</a>
				  </td>
				  <td>{{format-balance tx.amount}}</td>
				</tr>
			  {{/each}}
			</tbody>
		  </table>
		</div>
		{{else}}
		<h3>No payouts yet</h3>
		{{/if}}
	</div>
  </div>

<div class="note-note-info">
  {{#if model.payments}}
  <h4>Your Latest Payouts</h4>
  <div class="table table-responsive">
  <table class="table table-condensed table-striped">
    <thead>
      <tr>
        <th style="color:black">Time</th>
        <th style="color:black">Tx ID</th>
        <th style="color:black">Amount</th>
      </tr>
    </thead>
    <tbody>
      {{#each model.payments as |tx|}}
        <tr>
          <td style="color:black">{{format-date-locale tx.timestamp}}</td>
          <td>
            <a href="http://ubiqscan.io/en/tx/{{tx.tx}}" class="hash" rel="nofollow" target="_blank">{{tx.tx}}</a>
          </td>
          <td style="color:black">{{format-balance tx.amount}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
  {{else}}
  <h3>No payouts yet</h3>
  {{/if}}

</div>
</div>

